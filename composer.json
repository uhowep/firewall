{
    "name": "uhowep/firewall",
    "description": "A Simple Firewall For Laravel",
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "~5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "~5.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "uhowep",
            "email": "uhowep@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Uhowep\\Firewall\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Uhowep\\Firewall\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uhowep\\Firewall\\FirewallServiceProvider"
            ],
            "aliases": {
                "Firewall": "Uhowep\\Firewall\\Facades\\Firewall"
            }
        }
    }
}
